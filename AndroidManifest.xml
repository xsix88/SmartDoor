<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="itu.dk.masterthesis.smartdoor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="itu.dk.masterthesis.smartdoor.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="itu.dk.masterthesis.smartdoor.AdminActivity"
            android:label="@string/title_activity_admin" >
        </activity>
        <activity
            android:name="itu.dk.masterthesis.smartdoor.NoteActivity"
            android:label="@string/title_activity_note" >
        </activity>
        <activity
            android:name="itu.dk.masterthesis.smartdoor.FlickrActivity"
            android:label="@string/title_activity_flickr" >
        </activity>
        <activity
            android:name="itu.dk.masterthesis.smartdoor.ViewThumbnailsActivity"
            android:label="@string/title_activity_viewthumbnails" >
        </activity>
        
        
    </application>

</manifest>